#membuat array (list) berisi nama buah-buahan
buah = ["apel", "jeruk", "mangga", "pisang"]

#menambahkan elemen ke array
buah.append("semangka")

#mengakses elemen array
print("buah pertama:", buah[0])

#menampilkan semua isi array
for b in buah:
	print(b)
